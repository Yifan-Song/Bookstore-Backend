<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="BookStoreBackEnd.Entity.BookcacheEntity" table="bookcache" schema="booktable">
        <id name="cacheid" column="cacheid"/>
        <property name="userid" column="userid"/>
        <property name="bookpath" column="bookpath"/>
        <property name="status" column="status"/>
        <property name="bookname" column="bookname"/>
        <property name="price" column="price"/>
        <property name="author" column="author"/>
        <property name="year" column="year"/>
        <property name="bookid" column="bookid"/>
        <property name="number" column="number"/>
    </class>
</hibernate-mapping>